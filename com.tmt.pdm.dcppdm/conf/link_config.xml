<?xml version="1.0" encoding="euc-kr" ?>
<link_config>
	<!-- 
		화면의 오른쪽 부분에 나올 Class(Link할때 Target이 되는 class)와 해당 Class Package 지정 
		
		ex) Part
	-->
	<target>
		<classname>AbstractPart</classname>
		<packagename>Part</packagename>
		<fieldname>md$number</fieldname>
		<association>quantity</association>
	</target>

	<!-- 
		화면의 왼쪽 부분에 나올 Class(Link할때 Source이 되는 class)와 해당 Class Package 지정 
		ex) Model
	-->
	<source>
		<classname>FCADFile</classname>
		<packagename>Foundation</packagename>	
		<fieldname>md$number</fieldname>
		<association>Quantity</association>
	</source>
	
	
</link_config>