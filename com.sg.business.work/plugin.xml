<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="work"
            name="工作">
      </category>
      <view
            category="work"
            class="com.sg.widgets.part.view.AccountSensitiveTreeView"
            id="work.processing"
            name="待处理工作"
            restorable="true">
      </view>
   </extension>
   <extension
         point="com.sg.widgets.navigator">
      <navigator
            id="work.processing"
            tree="work.processing">
      </navigator>
   </extension>
   <extension
         point="com.sg.widgets.tree">
      <tree
            canCreate="false"
            canDelete="false"
            canEdit="false"
            canRead="true"
            dataSet="com.sg.business.model.dataset.work.ProcessingWork"
            editorId="editor.work"
            id="work.processing"
            popupMenu="popup:work.examine">
         <column
               columnDefinitionId="project.status">
         </column>
         <column
               columnDefinitionId="com.sg.widgets.commonlabel">
         </column>
         <structure
               modelRelation="work_work">
         </structure>
      </tree>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:work.processing">
         <command
               commandId="workprocessing.create"
               label="添加子工作"
               style="push">
         </command>
         <command
               commandId="workprocessing.createdeliverable"
               label="添加交付物"
               style="push">
         </command>
         <command
               commandId="workprocessing.linkdeliverable"
               icon="image/24_blank.png"
               label="链接项目的其他交付物..."
               style="push">
         </command>
         <separator
               name="workprocessing.separator0"
               visible="true">
         </separator>
         <command
               commandId="workprocessing.openproject"
               icon="image/24_blank.png"
               label="查看项目"
               style="push">
         </command>
         <command
               commandId="workprocessing.openwork"
               icon="image/work_24.png"
               label="查看工作"
               style="push">
         </command>
         <command
               commandId="workprocessing.opendeliverable"
               label="项目交付物"
               style="push">
         </command>
         <command
               commandId="workprocessing.edit"
               label="更改工作"
               style="push">
         </command>
         <menu
               icon="image/work2_24.png"
               label="工作操作">
            <command
                  commandId="workprocessing.start"
                  icon="image/work_start_24.png"
                  label="启动"
                  style="push">
            </command>
            <command
                  commandId="workprocessing.finish"
                  icon="image/work_finish_24.png"
                  label="完工"
                  style="push">
            </command>
            <command
                  commandId="workprocessing.pause"
                  icon="image/work_pause_24.png"
                  label="暂停"
                  style="push">
            </command>
            <command
                  commandId="workprocessing.cancel"
                  icon="image/work_cancel_24.png"
                  label="取消"
                  style="push">
            </command>
         </menu>
         <menu
               icon="image/execute_24.png"
               label="流程操作">
            <command
                  commandId="workprocessing.wfstart"
                  icon="image/project_start_24.png"
                  label="启动"
                  style="push">
            </command>
            <command
                  commandId="workprocessing.wffinish"
                  icon="image/project_finish_24.png"
                  label="完成"
                  style="push">
            </command>
            <command
                  commandId="workprocessing.wfcancel"
                  icon="image/project_stop_24.png"
                  label="中止"
                  style="push">
            </command>
         </menu>
         <command
               commandId="workprocessing.workrecord"
               label="编辑工作记录"
               style="push">
         </command>
         <separator
               name="workprocessing.separator1"
               visible="true">
         </separator>
         <menu
               icon="image/trash_24.png"
               label="删除">
            <command
                  commandId="workprocessing.delete"
                  label="删除子工作"
                  style="push">
            </command>
            <command
                  commandId="workprocessing.deletedeliverable"
                  label="删除交付物"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:work.examine">
         <command
               commandId="workprocessing.openproject"
               icon="image/24_blank.png"
               label="查看项目"
               style="push">
         </command>
         <command
               commandId="workprocessing.openwork"
               icon="image/work_24.png"
               label="查看工作"
               style="push">
         </command>
         <command
               commandId="workprocessing.opendeliverable"
               label="项目交付物"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="work"
            name="工作">
      </category>
      <command
            id="workprocessing.create"
            name="添加子工作">
      </command>
      <command
            id="workprocessing.createdeliverable"
            name="添加交付物">
      </command>
      <command
            id="workprocessing.linkdeliverable"
            name="链接项目的其他交付物...">
      </command>
      <command
            id="workprocessing.openproject"
            name="查看项目">
      </command>
      <command
            id="workprocessing.openwork"
            name="查看工作">
      </command>
      <command
            id="workprocessing.opendeliverable"
            name="项目交付物">
      </command>
      <command
            id="workprocessing.edit"
            name="更改工作">
      </command>
      <command
            categoryId="work"
            id="workprocessing.start"
            name="启动">
      </command>
      <command
            categoryId="work"
            id="workprocessing.finish"
            name="完成">
      </command>
      <command
            categoryId="work"
            id="workprocessing.pause"
            name="暂停">
      </command>
      <command
            categoryId="work"
            id="workprocessing.cancel"
            name="取消">
      </command>
      <command
            id="workprocessing.wfstart"
            name="启动">
      </command>
      <command
            id="workprocessing.wffinish"
            name="完成">
      </command>
      <command
            id="workprocessing.wfcancel"
            name="中止">
      </command>
      <command
            id="workprocessing.workrecord"
            name="编辑工作记录">
      </command>
      <command
            commandId="workprocessing.delete"
            id="workprocessing.delete"
            label="删除子工作"
            name="删除子工作"
            style="push">
      </command>
      <command
            id="workprocessing.deletedeliverable"
            name="删除交付物">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="workprocessing.create">
         <enabledWhen>
            <with
                  variable="selection">
               <with
                     variable="1">
               </with>
            </with>
         </enabledWhen>
      </handler>
      <handler
            commandId="workprocessing.createdeliverable">
         <enabledWhen>
            <with
                  variable="selection">
               <with
                     variable="1">
               </with>
            </with>
         </enabledWhen>
      </handler>
      <handler
            commandId="workprocessing.linkdeliverable">
         <enabledWhen>
            <with
                  variable="selection">
               <with
                     variable="1">
               </with>
            </with>
         </enabledWhen>
      </handler>
      <handler
            commandId="workprocessing.openproject">
         <enabledWhen>
            <with
                  variable="selection">
               <with
                     variable="1">
               </with>
            </with>
         </enabledWhen>
      </handler>
      <handler
            commandId="workprocessing.openwork">
         <enabledWhen>
            <with
                  variable="selection">
               <with
                     variable="1">
               </with>
            </with>
         </enabledWhen>
      </handler>
      <handler
            commandId="workprocessing.opendeliverable">
         <enabledWhen>
            <with
                  variable="selection">
               <with
                     variable="1">
               </with>
            </with>
         </enabledWhen>
      </handler>
      <handler
            commandId="workprocessing.edit">
         <enabledWhen>
            <with
                  variable="selection">
               <with
                     variable="1">
               </with>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="com.sg.business.commons.handler.LifeCycleActionStart"
            commandId="workprocessing.start">
         <enabledWhen>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false">
                  <test
                        args="start"
                        property="lifecycle.action">
                  </test>
               </iterate>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="com.sg.business.commons.handler.LifeCycleActionFinish"
            commandId="workprocessing.finish">
         <enabledWhen>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false">
                  <test
                        args="finish"
                        property="lifecycle.action">
                  </test>
               </iterate>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="com.sg.business.commons.handler.LifeCycleActionPause"
            commandId="workprocessing.pause">
         <enabledWhen>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false">
                  <test
                        args="pause"
                        property="lifecycle.action">
                  </test>
               </iterate>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="com.sg.business.commons.handler.LifeCycleActionCancel"
            commandId="workprocessing.cancel">
         <enabledWhen>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <test
                        args="cancel"
                        property="lifecycle.action">
                  </test>
               </iterate>
            </with>
         </enabledWhen>
      </handler>
      <handler
            commandId="workprocessing.wfstart">
         <enabledWhen>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false">
                  <test
                        args="start"
                        property="wf.action">
                  </test>
               </iterate>
            </with>
         </enabledWhen>
      </handler>
      <handler
            commandId="workprocessing.wffinish">
         <enabledWhen>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false">
                  <test
                        args="finish"
                        property="wf.action">
                  </test>
               </iterate>
            </with>
         </enabledWhen>
      </handler>
      <handler
            commandId="workprocessing.wfcancel">
         <enabledWhen>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <test
                        args="cancel"
                        property="wf.action">
                  </test>
               </iterate>
            </with>
         </enabledWhen>
      </handler>
      <handler
            commandId="workprocessing.workrecord">
         <enabledWhen>
            <with
                  variable="selection">
               <with
                     variable="1">
               </with>
            </with>
         </enabledWhen>
      </handler>
      <handler
            commandId="workprocessing.delete">
         <enabledWhen>
            <with
                  variable="selection">
               <with
                     variable="1">
               </with>
            </with>
         </enabledWhen>
      </handler>
      <handler
            commandId="workprocessing.deletedeliverable">
         <enabledWhen>
            <with
                  variable="selection">
               <with
                     variable="1">
               </with>
            </with>
         </enabledWhen>
      </handler>
   </extension>
   <extension
         point="com.sg.widgets.dataEditor">
      <editor
            collectionName="work"
            dbName="pm2"
            id="editor.work"
            image="image/work_24.png"
            isDefault="false"
            name="工作"
            titleToolTips="工作">
         <basicpage
               description="工作的基本信息"
               id="basicpage"
               image="image/work_24.png"
               title="基本信息">
            <pagecolumn
                  id="1">
               <section
                     label="基本信息">
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="desc"
                        label="名称"
                        labelVisible="true"
                        name="desc"
                        persistable="true"
                        required="false"
                        type="String"
                        widthHint="240">
                  </text>
                  <navigatorSelector
                        editable="true"
                        id="chargerid"
                        label="负责人"
                        labelVisible="true"
                        labelprovider="com.sg.business.commons.labelprovider.UserIdLabelProvider"
                        modelClass="com.sg.business.model.User"
                        name="chargerid"
                        navigatorId="project.participte.selector"
                        navigatorReturnField="userid"
                        persistable="true"
                        required="false">
                  </navigatorSelector>
                  <combo
                        editPart="Combo(Read Only)"
                        editable="false"
                        id="status"
                        label="状态"
                        labelVisible="true"
                        name="status"
                        option="status"
                        persistable="true"
                        required="false"
                        type="String">
                  </combo>
               </section>
               <section
                     label="参与者">
                  <list
                        addTableItemHandler="com.sg.business.project.editor.field.AddParticipate"
                        editable="true"
                        id="participate"
                        label="参与者"
                        labelVisible="false"
                        labelprovider="com.sg.business.commons.labelprovider.UserIdLabelProviderInField"
                        name="participate"
                        persistable="true"
                        required="false">
                  </list>
               </section>
            </pagecolumn>
            <pagecolumn
                  id="2">
               <section
                     label="计划">
                  <dateTime
                        editPart="DateDialog"
                        editable="true"
                        id="planstart"
                        label="计划开始"
                        labelVisible="true"
                        name="planstart"
                        persistable="true"
                        required="false"
                        type="Date">
                  </dateTime>
                  <dateTime
                        editPart="DateDialog"
                        editable="true"
                        id="planfinish"
                        label="计划完成"
                        labelVisible="true"
                        name="planfinish"
                        persistable="true"
                        required="false"
                        type="Date">
                  </dateTime>
                  <spinner
                        editable="true"
                        id="planduration"
                        label="计划工期(天)"
                        labelVisible="true"
                        name="planduration"
                        persistable="true"
                        required="false">
                  </spinner>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="standardworks"
                        label="标准工时(时)"
                        labelVisible="true"
                        name="standardworks"
                        persistable="true"
                        required="false"
                        type="Double">
                  </text>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="planworks"
                        label="计划工时(时)"
                        labelVisible="true"
                        name="planworks"
                        persistable="true"
                        required="false"
                        type="Double">
                  </text>
               </section>
               <section
                     label="实际">
                  <dateTime
                        editPart="DateDialog"
                        editable="false"
                        id="actualstart"
                        label="实际开始"
                        labelVisible="true"
                        name="actualstart"
                        persistable="true"
                        required="false"
                        type="Date">
                  </dateTime>
                  <dateTime
                        editPart="DateDialog"
                        editable="false"
                        id="actualfinish"
                        label="实际完成"
                        labelVisible="true"
                        name="actualfinish"
                        persistable="true"
                        required="false"
                        type="Date">
                  </dateTime>
                  <spinner
                        editable="false"
                        id="actualduration"
                        label="实际工期(天)"
                        labelVisible="true"
                        name="actualduration"
                        persistable="true"
                        required="false">
                  </spinner>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="actualworks"
                        label="实际工时(时)"
                        labelVisible="true"
                        name="actualworks"
                        persistable="true"
                        required="false"
                        type="Double">
                  </text>
               </section>
            </pagecolumn>
         </basicpage>
         <basicpage
               description="说明"
               id="memopage"
               image="image/work_24.png"
               title="说明">
            <pagecolumn>
               <section
                     label="说明">
                  <text
                        editPart="Text(Multi Line)"
                        editable="true"
                        heightHint="120"
                        id="description"
                        label="说明"
                        labelVisible="false"
                        name="description"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
               </section>
            </pagecolumn>
         </basicpage>
         <basicpage
               description="工作执行流程"
               id="processpage1"
               image="image/flow_24.png"
               pageDelegator="com.sg.business.work.editor.page.WorkProcessExecuteAssignmentPage"
               title="工作执行流程">
         </basicpage>
         <basicpage
               description="工作变更流程"
               id="processpage2"
               image="image/flow_24.png"
               pageDelegator="com.sg.business.work.editor.page.WorkProcessChangePage"
               title="工作变更流程">
         </basicpage>
      </editor>
   </extension>

</plugin>
