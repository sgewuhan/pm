<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="com.sg.widgets.option">
      <option
            id="文档编辑器"
            label="文档编辑器"
            value="文档编辑器">
         <option
               id="editor.document.generic"
               label="通用文档"
               value="editor.document.generic">
         </option>
         <option
               id="document.projectproposal"
               label="立项简表"
               value="document.projectproposal">
         </option>
      </option>
   </extension>
   <extension
         point="com.sg.widgets.dataEditor">
      <editor
            collectionName="documentd"
            dbName="pm2"
            id="editor.documentdefinition"
            image="image/documentd_24.png"
            name="文档模板"
            titleToolTips="文档模板">
         <basicpage
               description="文档模板"
               id="basicpage"
               image="image/documentd_24.png"
               title="文档模板">
            <pagecolumn
                  id="1">
               <section
                     label="基本信息">
                  <text
                        editPart="Text(Single Line)"
                        editable="false"
                        id="documentnumber"
                        label="编号"
                        labelVisible="true"
                        name="documentnumber"
                        persistable="true"
                        required="false"
                        textMessage="&lt;自动编号&gt;"
                        type="String">
                     <extendValueDefinition
                           type="com.sg.business.commons.numbering.DocumentTemplateNumber">
                     </extendValueDefinition>
                  </text>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="desc"
                        label="文档名称"
                        labelVisible="true"
                        name="desc"
                        persistable="true"
                        required="true"
                        type="String"
                        widthHint="240">
                  </text>
                  <combo
                        editPart="Combo(Read Only)"
                        editable="true"
                        id="document_editorid"
                        label="编辑器"
                        labelVisible="true"
                        name="document_editorid"
                        option="文档编辑器"
                        persistable="true"
                        required="false"
                        type="String">
                  </combo>
                  <text
                        editPart="Text(Multi Line)"
                        editable="true"
                        id="description"
                        label="说明"
                        labelVisible="true"
                        name="description"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
               </section>
            </pagecolumn>
         </basicpage>
         <basicpage
               id="basicpage2"
               image="image/documentd_24.png"
               title="模板文件">
            <pagecolumn>
               <section
                     label="模板文件">
                  <file
                        editPart="OfficeFileUploader"
                        editable="true"
                        id="templatefile"
                        label="模板文件"
                        labelVisible="false"
                        name="templatefile"
                        persistable="true"
                        required="false">
                  </file>
               </section>
            </pagecolumn>
         </basicpage>
         <versionControl>
            <fieldname
                  fieldname="$all">
            </fieldname>
         </versionControl>
      </editor>
      <pageExtendsions
            id="editor.documentdefinition"
            location="after=basicpage"
            targetId="systempage">
      </pageExtendsions>
   </extension>
   <extension
         point="com.sg.widgets.dataEditor">
      <editor
            collectionName="document"
            dbName="pm2"
            id="editor.document.generic"
            image="image/document_24.png"
            name="文档"
            titleToolTips="文档">
         <basicpage
               description="文档基本信息"
               id="basicpage1"
               image="image/document_24.png"
               title="基本信息">
            <pagecolumn
                  id="1">
               <section
                     label="基本属性">
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="documentnumber"
                        label="文档编号"
                        labelVisible="true"
                        name="documentnumber"
                        persistable="true"
                        required="true"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="desc"
                        label="文档名称"
                        labelVisible="true"
                        name="desc"
                        persistable="true"
                        required="true"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="documenttype"
                        label="文档类型"
                        labelVisible="true"
                        name="documenttype"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Multi Line)"
                        editable="true"
                        heightHint="300"
                        id="_summary"
                        label="摘要"
                        labelVisible="true"
                        name="_summary"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
               </section>
            </pagecolumn>
         </basicpage>
         <versionControl>
            <fieldname
                  fieldname="vault">
            </fieldname>
         </versionControl>
      </editor>
      <pageExtendsions
            id="editor.document.generic"
            location="after=basicpage1"
            targetId="systempage">
      </pageExtendsions>
      <pageExtendsions
            id="editor.document.generic"
            location="after=basicpage1"
            targetId="filepage">
      </pageExtendsions>
   </extension>
   <extension
         point="com.sg.widgets.dataEditor">
      <editor
            collectionName="document"
            dbName="pm2"
            id="document.projectproposal"
            image="image/document_24.png"
            name="立项简表"
            titleToolTips="立项简表">
         <basicpage
               description="文档基本信息"
               id="basicpage1"
               image="image/document_24.png"
               title="基本信息">
            <pagecolumn
                  id="1">
               <section
                     label="基本属性">
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="documentnumber"
                        label="文档编号"
                        labelVisible="true"
                        name="documentnumber"
                        persistable="true"
                        required="true"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="desc"
                        label="文档名称"
                        labelVisible="true"
                        name="desc"
                        persistable="true"
                        required="true"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="documenttype"
                        label="文档类型"
                        labelVisible="true"
                        name="documenttype"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Multi Line)"
                        editable="true"
                        heightHint="300"
                        id="_summary"
                        label="摘要"
                        labelVisible="true"
                        name="_summary"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
               </section>
            </pagecolumn>
         </basicpage>
         <versionControl>
            <fieldname
                  fieldname="vault">
            </fieldname>
         </versionControl>
      </editor>
      <pageExtendsions
            id="document.projectproposal"
            location="after=basicpage1"
            targetId="systempage">
      </pageExtendsions>
      <pageExtendsions
            id="document.projectproposal"
            location="after=basicpage1"
            targetId="filepage">
      </pageExtendsions>
   </extension>

</plugin>
