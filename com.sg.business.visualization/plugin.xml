            <column
                  column="field2"
                  name="字段2"
                  type="Boolean">
            </column>
         </filterAction>
         <searchAction>
            <column
                  column="field1"
                  name="字段1"
                  type="String">
            </column>
            <column
                  column="field2"
                  name="字段2"
                  type="Boolean">
            </column>
         </searchAction>
      </navigator>
      <navigator
            id="visualization.demomodel2"
            provideSelection="true"
