      <table
            customItemHeight="40"
            dataSet="com.sg.business.model.dataset.project.PMOrgOfPM"
            id="project.managementorg"
            markupEnabled="true">
         <column
               columnDefinitionId="organization.desc.html">
         </column>
      </table>
      <table
            customItemHeight="40"
            dataSet="com.sg.business.model.dataset.project.LaunchOrgOfPM"
            id="project.launchorg"
            markupEnabled="true">
         <column
               columnDefinitionId="organization.desc.html">
         </column>
      </table>
   </extension>
   <extension
