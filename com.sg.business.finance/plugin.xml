<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="com.mobnut.admin.adminFunction">
      <adminFunction
            id="com.sg.business.finance.admin.budgetitem"
            image="image/budgetitem_48.png"
            label="%adminFunction.label"
            run="com.sg.business.finance.admin.BudgetItemFunction">
      </adminFunction>
      <adminFunction
            id="com.sg.business.finance.admin.DevAccountNumberSetting"
            image="image/item_48.png"
            label="%adminFunction.label.0"
            run="com.sg.business.finance.admin.DevAccountNumberSetting"
            tooltip="%adminFunction.tooltip">
      </adminFunction>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.sg.business.finance.editor.BudgetItemEditor"
            default="false"
            id="com.sg.business.finance.budgetitemeditor"
            name="%editor.name">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.sg.widgets.part.view.TableNavigator"
            id="financial.accountnumber"
            name="%view.name"
            restorable="true">
      </view>
      <view
            class="com.sg.widgets.part.view.TableNavigator"
            id="finance.org.navigator"
            name="%view.name.0"
            restorable="true">
      </view>
      <view
            class="com.sg.business.finance.rndcost.RNDCostAdjustmentView"
            id="finance.rndcost"
            name="%view.name.1"
            restorable="true">
      </view>
   </extension>
   <extension
         point="com.sg.widgets.navigator">
      <navigator
            id="financial.accountnumber"
            table="financial.accountnumber">
         <filterAction>
            <column
                  column="desc"
                  name="%column.name"
                  type="String">
            </column>
            <column
                  column="accountnumber"
                  name="%column.name.0"
                  type="String">
            </column>
         </filterAction>
         <searchAction>
            <column
                  column="desc"
                  name="%column.name.1"
                  type="String">
            </column>
            <column
                  column="accountnumber"
                  name="%column.name.2"
                  type="String">
            </column>
         </searchAction>
         <exportAction>
            <column
                  column="desc"
                  name="%column.name.3"
                  type="String">
            </column>
            <column
                  column="accountnumber"
                  name="%column.name.4"
                  type="String">
            </column>
         </exportAction>
         <importAction>
            <column
                  column="desc"
                  isKey="true"
                  name="%column.name.5"
                  type="String">
            </column>
            <column
                  column="accountnumber"
                  isKey="true"
                  name="%column.name.6"
                  type="String">
            </column>
         </importAction>
      </navigator>
      <navigator
            id="finance.org.navigator"
            table="finance.org.navigator">
      </navigator>
   </extension>
   <extension
         point="com.sg.widgets.table">
      <table
            collectionName="costaccount"
            dataSet="com.sg.business.model.dataset.financial.CostAccountSetting"
            dbName="pm2"
            editorId="editor.devaccountnumber"
            id="financial.accountnumber"
            popupMenu="popup:com.sg.widget.navigator">
         <column
               column="desc"
               name="%column.name.7"
               type="String"
               width="120">
         </column>
         <column
               column="accountnumber"
               name="%column.name.8"
               type="String"
               width="120">
         </column>
         <column
               column="budgetitem"
               name="%budgetitem"
               type="String"
               width="120">
         </column>
      </table>
      <table
            canCreate="false"
            canDelete="false"
            canEdit="false"
            canRead="false"
            dataSet="com.sg.business.model.dataset.finance.FinanceOrganizationDataSet"
            id="finance.org.navigator">
         <column
               column="fulldesc"
               name="%column.name.9"
               style="SWT.LEFT"
               type="String"
               width="-1">
         </column>
         <column
               columnDefinitionId="organization.costCenterCode">
         </column>
         <column
               column="companycode"
               name="%column.name.10"
               type="String">
         </column>
      </table>
   </extension>
   <extension
         point="com.sg.widgets.dataEditor">
      <editor
            collectionName="costaccount"
            dbName="pm2"
            id="editor.devaccountnumber"
            name="%editor.name">
         <basicpage
               id="basicpage1"
               title="%basicpage.title">
            <pagecolumn>
               <section
                     label="%section.label">
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="desc"
                        label="%text.label"
                        labelVisible="true"
                        name="desc"
                        persistable="true"
                        required="true"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="accountnumber"
                        label="%text.label.0"
                        labelVisible="true"
                        name="accountnumber"
                        persistable="true"
                        required="true"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="budgetitem"
                        label="%budgetitem"
                        labelVisible="true"
                        name="budgetitem"
                        persistable="true"
                        required="true"
                        type="String">
                  </text>
               </section>
            </pagecolumn>
         </basicpage>
      </editor>
   </extension>

</plugin>
