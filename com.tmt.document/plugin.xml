<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="com.sg.widgets.dataEditor">
      <editor
            collectionName="document"
            dbName="pm2"
            id="document.prodectproposal"
            name="新产品开发建议及立项书"
            titleToolTips="新产品开发建议及立项书">
         <basicpage
               description="基本信息"
               id="page1"
               title="基本信息">
            <pagecolumn
                  id="1">
               <section
                     label="基本属性">
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="projectdesc"
                        label="项目名称"
                        labelVisible="true"
                        name="projectdesc"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="workorder"
                        label="工作令号"
                        labelVisible="true"
                        name="workorder"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
               </section>
               <section
                     label="用户信息">
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="customerdesc"
                        label="单位名称"
                        labelVisible="true"
                        name="customerdesc"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="customerlinkman"
                        label="联系人"
                        labelVisible="true"
                        name="customerlinkman"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="customertel"
                        label="电话"
                        labelVisible="true"
                        name="customertel"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="customerfax"
                        label="传真"
                        labelVisible="true"
                        name="customerfax"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Single Line)"
                        editable="true"
                        id="customermail"
                        label="E-mail"
                        labelVisible="true"
                        name="customermail"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
               </section>
               <section
                     label="样品来源">
                  <text
                        editPart="Text(Multi Line)"
                        editable="true"
                        heightHint="4"
                        id="samplesource"
                        label="样品来源"
                        labelVisible="false"
                        name="samplesource"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
               </section>
               <section
                     label="项目简介及市场预测">
                  <text
                        editPart="Text(Multi Line)"
                        editable="true"
                        heightHint="6"
                        id="projectdescription"
                        label="项目简介及市场预测"
                        labelVisible="false"
                        name="projectdescription"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
               </section>
               <section
                     label="项目要求">
                  <text
                        editPart="Text(Multi Line)"
                        editable="true"
                        heightHint="3"
                        id="technicalrequirements"
                        label="产品技术要求（包括功能、性能、尺寸及特殊特性等）："
                        labelVisible="true"
                        name="technicalrequirements"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Multi Line)"
                        editable="true"
                        heightHint="3"
                        id="packagerequirements"
                        label="产品包装/标识要求"
                        labelVisible="true"
                        name="packagerequirements"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
                  <text
                        editPart="Text(Multi Line)"
                        editable="true"
                        heightHint="3"
                        id="schedulerequirements"
                        label="进度要求"
                        labelVisible="true"
                        name="schedulerequirements"
                        persistable="true"
                        required="false"
                        type="String">
                  </text>
               </section>
            </pagecolumn>
         </basicpage>
         <versionControl>
            <fieldname
                  fieldname="vault">
            </fieldname>
         </versionControl>
      </editor>
   </extension>

</plugin>
