         point="com.sg.widgets.dataEditor">
      <editor
            collectionName="taskform"
            dbName="pm2"
            id="pm.systemprojectapprove.audit"
            name="系统部评审审批">
         <basicpage
               id="basicpage1"
               title="审批">
            <pagecolumn>
               <section
                     label="请填写批准意见、结论和评审专家组成员">
                  <text
                        editPart="Text(Multi Line)"
                        editable="true"
                        id="comment"
                        label="意见"
                        labelVisible="false"
                        name="comment"
                        persistable="true"
                        required="false"
                        type="String"
                        widthHint="320">
                  </text>
                  <radio
                        editable="true"
                        id="choice"
                        label="批准"
                        labelVisible="false"
                        name="choice"
                        option="是否通过"
                        persistable="true"
                        required="false"
                        type="String">
                  </radio>
                  <radio
                        editable="true"
                        id="pretrial"
                        label="资料预审"
                        labelVisible="false"
                        name="pretrial"
                        option="是否资料预审"
                        persistable="true"
                        required="false"
                        type="String">
                  </radio>
                  <navigatorSelector
                        editable="true"
                        id="reviewer_admin"
                        label="评审组组长"
                        labelVisible="true"
                        labelprovider="com.sg.business.commons.labelprovider.UserIdLabelProvider"
                        modelClass="com.sg.business.model.User"
                        name="reviewer_admin"
                        navigatorId="project.participte.selector"
                        navigatorReturnField="userid"
                        persistable="true"
                        required="true">
                  </navigatorSelector>
                  <list
                        addTableItemHandler="com.sg.business.commons.field.action.AddParticipate"
                        editable="true"
                        id="reviewer_list"
                        label="评审组"
                        labelVisible="true"
                        labelprovider="com.sg.business.commons.labelprovider.UserIdLabelProviderInField"
                        name="reviewer_list"
                        persistable="true"
                        required="false">
                  </list>
               </section>
            </pagecolumn>
         </basicpage>
      </editor>
