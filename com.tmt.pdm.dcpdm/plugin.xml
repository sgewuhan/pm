<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <!--extension
         point="com.mobnut.admin.schedualJob">
      <schedualJob
            day="99999"
            id="dcppdm.importkh"
            name="临时维护"
            rule="month"
            run="com.tmt.pdm.dcpdm.sync.ImportKH"
            time="0:0:0">
      </schedualJob>
   </extension-->
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:project.deliverable?after=project.linkdeliverable">
         <command
               commandId="deliverable.linkpdm"
               icon="image/rep_document_24.png"
               label="%command.label"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false">
                     <and>
                        <instanceof
                              value="com.sg.business.model.Work">
                        </instanceof>
                        <test
                              args="createDeliverable"
                              property="runtimework.action">
                        </test>
                     </and>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:workflow.workdelivery?after=additions">
         <command
               commandId="workflow.deliverable.linkpdm"
               label="%command.label.0"
               mode="FORCE_TEXT"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="deliverable.linkpdm"
            name="%command.name">
      </command>
      <command
            id="workflow.deliverable.linkpdm"
            name="%command.name.0">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.tmt.pdm.dcpdm.handler.LinkPDMDocAndDraw"
            commandId="deliverable.linkpdm">
      </handler>
      <handler
            class="com.tmt.pdm.dcpdm.handler.LinkPDMDocAndDraw2"
            commandId="workflow.deliverable.linkpdm">
      </handler>
   </extension>
   <extension
         point="com.sg.widgets.dataEditor">
      <basicpage
            id="pdm.vault"
            pageDelegator="com.tmt.pdm.dcpdm.page.PDMVaultPage"
            title="PDM">
      </basicpage>
      <editor
            collectionName="document"
            dbName="pm2"
            id="editor.document.dcpdm"
            name="DCPDM图文档">
         <basicpage
               description="DCPDM图文档基本信息"
               id="basicpage1"
               pageDelegator="com.tmt.pdm.dcpdm.editor.DocumentPage"
               title="基本信息">
            <pagecolumn
                  id="1">
               <section
                     label="基本">
                  <fieldgroup
                        columnCount="3">
                     <text
                           editPart="Text(Single Line)"
                           editable="false"
                           id="documentnumber"
                           label="文档编号"
                           labelVisible="true"
                           name="documentnumber"
                           persistable="true"
                           required="true"
                           type="String">
                     </text>
                     <text
                           editPart="Text(Single Line)"
                           editable="false"
                           id="desc"
                           label="文档名称"
                           labelVisible="true"
                           name="desc"
                           persistable="true"
                           required="true"
                           type="String">
                     </text>
                     <text
                           editPart="Text(Single Line)"
                           editable="false"
                           id="pdm_ouid"
                           label="PDM内部代码"
                           labelVisible="true"
                           name="pdm_ouid"
                           persistable="true"
                           required="false"
                           type="String">
                        <fieldaction
                              handler="com.tmt.pdm.dcpdm.handler.SelectPDMObject"
                              image="image/find_16.png"
                              name="选择">
                        </fieldaction>
                     </text>
                  </fieldgroup>
               </section>
            </pagecolumn>
         </basicpage>
      </editor>
      <pageExtendsions
            id="editor.document.dcpdm"
            location="after=basicpage1"
            targetId="document.systempage">
      </pageExtendsions>
      <pageExtendsions
            id="editor.document.dcpdm"
            location="after=basicpage1"
            targetId="document.wfhistory">
      </pageExtendsions>
   </extension>
   <extension
         point="com.sg.business.model.fileserver">
      <fileserver
            document="document"
            fileServerDelegator="com.tmt.pdm.dcpdm.FileServerDelegator">
      </fileserver>
   </extension>

</plugin>
